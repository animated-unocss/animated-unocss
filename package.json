{
  "name": "@animated-unocss/monorepo",
  "private": "true",
  "description": "A configuration to use Animate.css with UnoCSS.",
  "author": "Elone Hoo <huchengyea@163.com>",
  "type": "module",
  "license": "MIT",
  "version": "0.0.0",
  "scripts": {
    "prepare": "pnpm -r run stub",
    "release": "bumpp package.json core/package.json --commit --push --tag && pnpm -r publish --access public",
    "typeCheck": "tsc --noEmit",
    "test": "pnpm -C test run test",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@elonehoo/eslint-config": "2.0.0",
    "@elonehoo/rimraf": "0.0.1",
    "@types/node": "18.8.3",
    "bumpp": "8.2.1",
    "eslint": "8.24.0",
    "pnpm": "7.13.2",
    "typescript": "4.8.4",
    "vite": "3.1.6"
  }
}
